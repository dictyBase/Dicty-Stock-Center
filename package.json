{
  "name": "Dicty-Stock-Center",
  "version": "0.0.17",
  "description": "The Dicty Stock Center app rebuilt with React and Redux!",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/components/*.js",
      "!<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "styles": "<rootDir>/src/styles/index.js"
    },
    "roots": ["<rootDir>/src/"]
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "server": "node chuck-norris-server/server.js",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy": "better-npm-run deploy",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon -r dotenv/config bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node -r dotenv/config bin/server"
    },
    "test": {
      "command": "jest --coverage",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/Dicty-Stock-Center.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "debug": "^2.2.0",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#v2.1.0",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar",
    "dicty-components-panel": "github:dictyBase/dicty-components-panel",
    "draft-js": "^0.10.1",
    "draft-js-plugins-editor": "^2.0.0-beta1",
    "draft-js-static-toolbar-plugin": "^2.0.0-rc8",
    "draft-js-toolbar-link-plugin": "^1.0.1",
    "draft-js-undo-plugin": "^2.0.0-rc8",
    "es6-promise": "^3.2.1",
    "font-awesome": "^4.7.0",
    "fs": "0.0.2",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jsrsasign": "^5.0.5",
    "lodash": "^4.15.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^15.1.0",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-virtualized": "^8.5.3",
    "reactabular": "^2.0.2",
    "rebass": "^1.0.4",
    "redux": "^3.7.2",
    "redux-form": "^5.2.5",
    "redux-thunk": "^2.1.0",
    "simplestorage.js": "^0.2.1",
    "styled-components": "^2.2.3",
    "url": "^0.11.0",
    "validator": "^5.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-jest": "^22.0.3",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.26.0",
