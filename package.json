{
  "name": "dicty-stock-center",
  "version": "0.0.0-semantically-released",
  "description": "The Dicty Stock Center app rebuilt with React and Redux!",
  "main": "index.js",
  "homepage": "/stockcenter",
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext .jsx,.js src/",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "nightwatch": "nightwatch -e chrome,firefox,safari",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "local-ci-eric": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p ericdev",
    "local-ci-sidd": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p devsidd",
    "local-ci-staging": "IMAGE_TAG=staging-$(git rev-parse --short HEAD) skaffold run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/Dicty-Stock-Center.git"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^4.5.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-link-persisted-queries": "^0.2.2",
    "bowser": "^1.9.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.1.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-redux": "github:dictybase-playground/dicty-components-redux#v2.3.0",
    "draft-js": "^0.10.5",
    "draft-js-plugins-editor": "^2.0.4",
    "draft-js-static-toolbar-plugin": "^2.0.1",
    "draft-js-toolbar-link-plugin": "^1.0.1",
    "draft-js-undo-plugin": "^2.0.2",
    "element-remove": "^1.0.4",
    "file-loader": "^4.1.0",
    "formik": "^1.5.8",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "jwt-decode": "^2.2.0",
    "react": "^16.10.2",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.10.2",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-loadable": "^5.5.0",
    "react-loading-skeleton": "^1.1.2",
    "react-redux": "^7.1.0",
    "react-responsive-carousel": "^3.1.49",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "string.prototype.includes": "^1.0.0",
    "typeface-roboto": "0.0.75",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.16",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.8.0",
    "babel-preset-react-app": "^9.0.0",
    "chromedriver": "^75.1.0",
    "commitizen": "^4.0.2",
    "cz-conventional-changelog": "^3.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.106.0",
    "geckodriver": "^1.16.2",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "jss": "^9.8.7",
    "lint-staged": "^9.4.1",
    "nightwatch": "^1.0.6",
    "nock": "^11.4.0",
    "prettier": "^1.17.1",
    "react-styleguidist": "^9.1.11",
    "react-test-renderer": "^16.10.2",
    "redux-mock-store": "^1.5.3",
    "selenium-server": "^3.13.0",
    "semantic-release": "^15.13.23",
    "sinon": "^7.3.2",
    "source-map-explorer": "^2.0.1",
    "waait": "^1.0.5"
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && CI=true npm run test",
      "post-commit": "npm run local-ci-eric"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "ci": false,
    "tagFormat": "${version}",
    "verifyConditions": [
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
