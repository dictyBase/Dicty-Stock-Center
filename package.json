  "devDependencies": {
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.8.0",
    "babel-preset-react-app": "^3.1.2",
    "chromedriver": "^2.40.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-config-react-app": "^4.0.0",
