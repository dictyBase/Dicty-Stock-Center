{
  "name": "dicty-stock-center",
  "version": "0.0.0-semantically-released",
  "description": "The Dicty Stock Center app rebuilt with React and Redux!",
  "main": "index.js",
  "engines": {
    "node": "10.17.0"
  },
  "homepage": "/stockcenter",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "CI=true react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext .jsx,.js src/",
    "tsc": "tsc",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "local-ci-eric": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p ericdev",
    "local-ci-sidd": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p devsidd",
    "cypress:open": "cypress open",
    "cypress:run": "percy exec -- cypress run",
    "cypress:ci": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/Dicty-Stock-Center.git"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.9.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-link-persisted-queries": "^0.2.2",
    "bowser": "^1.9.4",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.1.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "draft-js": "^0.10.5",
    "draft-js-plugins-editor": "^2.0.4",
    "draft-js-static-toolbar-plugin": "^2.0.1",
    "draft-js-toolbar-link-plugin": "^1.0.1",
    "draft-js-undo-plugin": "^2.0.2",
    "element-remove": "^1.0.4",
    "formik": "^2.1.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "history": "^4.10.1",
    "jwt-decode": "^2.2.0",
    "querystring": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-loading-skeleton": "^2.0.1",
    "react-responsive-carousel": "^3.1.51",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "string.prototype.includes": "^1.0.0",
    "typeface-roboto": "0.0.75",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@percy/cypress": "^2.3.0",
    "@semantic-release/git": "^7.0.18",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/draft-js": "^0.10.38",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.5",
    "@types/jest": "^24.0.25",
    "@types/jwt-decode": "^2.2.1",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.1",
    "@types/react-window-infinite-loader": "^1.0.2",
    "@types/sprintf-js": "^1.1.2",
    "@types/testing-library__react-hooks": "^3.2.0",
    "apollo-boost": "^0.4.7",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.9.0",
    "babel-preset-react-app": "^9.1.1",
    "commitizen": "^4.0.3",
    "core-js": "^3.6.4",
    "cypress": "^4.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "flow-bin": "^0.118.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^9.5.0",
    "nock": "^11.8.2",
    "prettier": "^1.19.1",
    "react-docgen-typescript": "^1.16.2",
    "react-styleguidist": "^10.6.2",
    "react-test-renderer": "^16.12.0",
    "semantic-release": "^15.14.0",
    "sinon": "^8.1.1",
    "source-map-explorer": "^2.3.1",
    "sprintf-js": "^1.1.2",
    "start-server-and-test": "^1.10.8",
    "typescript": "^3.7.5",
    "waait": "^1.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/hooks/**/*.{js,jsx,ts,tsx}",
      "src/utils/redirectUrlGenerator.ts",
      "src/utils/strainOrPlasmid.ts",
      "!src/components/**/mock*.{js,jsx,ts,tsx}",
      "!src/components/**/types.{js,jsx,ts,tsx}",
      "!src/components/**/*Styles.{js,jsx,ts,tsx}"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "ci": false,
    "tagFormat": "${version}",
    "verifyConditions": [
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
