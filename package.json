{
  "name": "dicty-stock-center",
  "version": "0.0.0-semantically-released",
  "description": "The Dicty Stock Center app rebuilt with React and Redux!",
  "main": "index.js",
  "engines": {
    "node": "10.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "nightwatch": "nightwatch -e chrome,firefox,safari",
    "precommit": "lint-staged",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/Dicty-Stock-Center.git"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^3.9.3",
    "apollo-boost": "^0.3.1",
    "bowser": "^1.9.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.3.0",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#v7.1.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#v1.4.0",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#v0.2.0",
