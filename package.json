{
  "name": "Dicty-Stock-Center",
  "version": "0.0.17",
  "description": "The Dicty Stock Center app rebuilt with React and Redux!",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "server": "node chuck-norris-server/server.js",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon -r dotenv/config bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node -r dotenv/config bin/server"
    },
    "test": {
      "command": "node -r dotenv/config ./node_modules/karma/bin/karma start bin/karma.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/Dicty-Stock-Center.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "classnames": "^2.2.3",
    "debug": "^2.2.0",
    "dicty-react-components": "git://github.com/dictyBase/dicty-react-components.git#refactor/inline-style",
    "draft-js": "^0.7.0",
    "es6-promise": "^3.2.1",
    "fs": "0.0.2",
    "history": "^2.1.2",
    "isomorphic-fetch": "^2.2.1",
    "jsrsasign": "^5.0.5",
    "lodash": "^4.15.0",
    "radium": "^0.17.1",
    "radium-grid": "^2.0.1",
    "react": "^15.1.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^15.1.0",
    "react-ga": "^2.1.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.5",
    "react-virtualized": "^8.5.3",
    "reactabular": "^2.0.2",
    "redux": "^3.0.0",
    "redux-form": "^5.2.5",
    "redux-thunk": "^2.1.0",
    "simplestorage.js": "^0.2.1",
    "styled-components": "^1.4.6",
    "url": "^0.11.0",
    "validator": "^5.1.0"
  },
